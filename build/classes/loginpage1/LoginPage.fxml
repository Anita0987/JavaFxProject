<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="loginpage1.LoginPageController">
   <children>
      <Label layoutX="137.0" layoutY="8.0" text="Student Management System" textFill="#754141">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
      <GridPane layoutX="100.0" layoutY="90.0" prefHeight="236.0" prefWidth="423.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="75.0" minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="69.0" minHeight="10.0" prefHeight="68.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label prefHeight="35.0" prefWidth="151.0" text="UserName">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
            <Label prefHeight="35.0" prefWidth="167.0" text="Password" GridPane.rowIndex="1">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
            <Label prefHeight="38.0" prefWidth="179.0" text="LoginType" GridPane.rowIndex="2">
               <font>
                  <Font size="23.0" />
               </font>
            </Label>
            <TextField fx:id="txtUsername" prefHeight="33.0" prefWidth="211.0" promptText="Username" GridPane.columnIndex="1">
               <font>
                  <Font size="18.0" />
               </font>
            </TextField>
            <PasswordField fx:id="txtPassword" prefHeight="32.0" prefWidth="211.0" promptText="Password" GridPane.columnIndex="1" GridPane.rowIndex="1">
               <font>
                  <Font size="18.0" />
               </font>
            </PasswordField>
            <ComboBox fx:id="comboBox" prefHeight="39.0" prefWidth="211.0" promptText="LoginType" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <Button fx:id="btnLogin" mnemonicParsing="false" onAction="#handleLoginButtonPressed" prefHeight="51.0" prefWidth="186.0" text="Login" GridPane.rowIndex="3">
               <font>
                  <Font size="24.0" />
               </font>
            </Button>
         </children>
      </GridPane>
   </children>
</AnchorPane>
